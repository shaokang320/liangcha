            align-content: space-between;
    -webkit-flex-wrap: wrap;
        -ms-flex-wrap: wrap;
            flex-wrap: wrap;
}
.section-b .list .item {
      width: 138rpx;
      height: 138rpx;
      margin-top: 21rpx;
      background: #ffffff;
      border-radius: 10rpx;
      text-align: center;
      line-height: 138rpx;
      color: #000000;
      border: 1px solid #E5E5E5;
}
.section-b .list .active {
      color: #ffffff;
      background: #00a47c;
      border: 1px solid #00a47c;
}
