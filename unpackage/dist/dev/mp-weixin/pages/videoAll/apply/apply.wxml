<view class="content"><view class="section-a c690"><view class="only"><text class="only-l">姓名</text><input class="only-r" type="text" placeholder="输入" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" value="{{name}}" bindinput="__e"/></view><picker class="box" mode="selector" range="{{gender}}" value="{{genderIndex}}" data-event-opts="{{[['change',[['changeGender',['$event']]]]]}}" bindchange="__e"><view class="h-title"><text>性别</text><view class="r"><text>{{gender[genderIndex]}}</text></view></view></picker><view class="only"><text class="only-l">手机号</text><input class="only-r" type="text" placeholder="输入" data-event-opts="{{[['input',[['__set_model',['','tel','$event',[]]]]]]}}" value="{{tel}}" bindinput="__e"/></view><view class="only"><text class="only-l">身份证号</text><input class="only-r" type="text" placeholder="输入" data-event-opts="{{[['input',[['__set_model',['','card','$event',[]]]]]]}}" value="{{card}}" bindinput="__e"/></view><view class="cart-title"><text class="l">上传您的身份证照片</text><text class="r">（手持身份证正面）</text></view><view data-event-opts="{{[['tap',[['photograph',['$event']]]]]}}" class="card-img c660" bindtap="__e"><image src="{{cartImg?cartImg:staticURL+'videoAll/idCard.png'}}" mode="widthFix"></image></view></view><button data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="btn c660" bindtap="__e">提交申请</button></view>