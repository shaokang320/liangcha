<view class="content"><view class="section-a c690"><view data-event-opts="{{[['tap',[['changeTime',['$event']]]]]}}" class="h-title c690" bindtap="__e"><text>选择时间</text><text class="time">{{day+" "+time}}</text><image src="{{staticURL+'common/arrow-gray.png'}}"></image></view></view><view class="section-c c690"><scroll-view class="list" scroll-y="true"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="i" wx:key="i"><view data-event-opts="{{[['tap',[['goPage',['/pages/cookAll/cookDetail/cookDetail']]]]]}}" class="item" bindtap="__e"><view class="pic"><image src="{{staticURL+item.pic}}"></image></view><view class="detail"><view class="name"><text>{{item.name}}</text></view><view class="desc">{{''+item.desc+": "+item.num+''}}</view></view><view class="specification"><view>{{'￥'+item.price+''}}</view></view></view></block></scroll-view></view><mask class="{{['components-mask',flag?'':'none']}}" vue-id="1" bgcolor="#ffffff" bind:__l="__l" vue-slots="{{['default']}}"><view class="c690"><uni-calendar vue-id="{{('2')+','+('1')}}" disableBefore date="{{startData}}" insert="{{true}}" lunar="{{true}}" start-date="{{startData}}" end-date="{{endData}}" data-event-opts="{{[['^change',[['change']]]]}}" bind:change="__e" bind:__l="__l"></uni-calendar></view><view class="time c690"><view class="common-title"><view class="t-l">时间</view><view class="t-r"><picker mode="time" value="{{time}}" start="{{startTime}}" end="{{endTime}}" data-event-opts="{{[['change',[['bindTimeChange',['$event']]]]]}}" bindchange="__e"><view class="text">{{time}}</view></picker><image src="{{staticURL+'common/arrow-gray.png'}}"></image></view></view></view><button class="btn" type="primary" data-event-opts="{{[['tap',[['changeTime',['$event']]]]]}}" bindtap="__e">确认</button></mask></view>